{
  "manifest_version": 3,
  "name": "Extension",
  "version": "1.0",
  "permissions": [
      "storage",
      "activeTab"
  ],
  "background": {
      "service_worker": "background.js"
  },
  "content_scripts": [
      {
          "matches": ["<all_urls>"],
          "js": ["Content.js"]
      }
  ],
  "browser_action": {
      "default_popup": "popup.html"
  }
}